{
  "kind": "collectionType",
  "collectionName": "institutions",
  "info": {
    "singularName": "institution",
    "pluralName": "institutions",
    "displayName": "/ Instituicao",
    "description": "Educational institutions in the Grupo SER network"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nome": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "descricao": {
      "type": "text"
    },
    "cidadePadrao": {
      "type": "string",
      "required": false
    },
    "cursos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course",
      "mappedBy": "instituicao"
    },
    "home_pergunta_frequente": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::pergunta-frequente.pergunta-frequente",
      "inversedBy": "instituicoes"
    }
  }
}
